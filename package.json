{
  "name": "commentify",
  "displayName": "Commentify",
  "description": "Turbocharge Your Code with AI-Generated Comments!",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.addCommentsAboveFunctions"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addCommentsAboveFunctions",
        "title": "Add Comments Above Functions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/divyamsavsaviya/Commentify"
  },
  "keybindings": [
    {
      "command": "extension.addCommentsAboveFunctions",
      "key": "ctrl+shift+c",
      "mac": "cmd+shift+c",
      "when": "editorTextFocus"
    }
  ],
  "dependencies": {
    "dotenv": "^16.1.3"
  }
}
